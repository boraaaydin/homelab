# Docker Configuration
DOCKER_TAG=latest

# Domain Configuration (for domain-based access)
DOMAIN_PREFIX=agent-zero
DOMAIN=example.com

# Localhost Configuration (for port-based access)
HOST_PORT=50001

# System Configuration
TZ=UTC

# LLM API Configuration

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Claude
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# DeepSeek
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Google Gemini
GOOGLE_API_KEY=your_google_api_key_here

# Groq
GROQ_API_KEY=your_groq_api_key_here

# HuggingFace
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# LM Studio (local)
LM_STUDIO_BASE_URL=http://host.docker.internal:1234

# Mistral AI
MISTRAL_API_KEY=your_mistral_api_key_here

# Ollama (local LLM)
OLLAMA_BASE_URL=http://host.docker.internal:11434

# Azure OpenAI
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/

# OpenRouter
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Sambanova
SAMBANOVA_API_KEY=your_sambanova_api_key_here

# Venice.ai
VENICE_API_KEY=your_venice_api_key_here

# xAI (Grok)
XAI_API_KEY=your_xai_api_key_here

# Other OpenAI compatible APIs
OPENAI_COMPATIBLE_BASE_URL=your_custom_openai_compatible_endpoint
OPENAI_COMPATIBLE_API_KEY=your_custom_api_key_here