# Minio Environment Variables
MINIO_ROOT_USER=your_username
MINIO_ROOT_PASSWORD=your_password
MINIO_DEFAULT_BUCKETS=your_bucket_name

# Docker Image
DOCKER_IMAGE=docker.io/bitnami/minio
DOCKER_TAG=2025.5.24
DOCKER_CLIENT_IMAGE=minio/mc
DOCKER_CLIENT_TAG=latest

# Traefik Configuration
DOMAIN_PREFIX=minio
MINIO_CONSOLE_DOMAIN_PREFIX=minio-console
DOMAIN=your.domain.com

# Optional: Host port for direct access (e.g., 9000 for Minio API, 9001 for Minio Console)
# If you set this, create docker-compose.ports.yml accordingly
# Example for Minio API and Console:
# HOST_PORT_API=9000
# HOST_PORT_CONSOLE=9001
HOST_PORT= 